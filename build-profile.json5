{
  "app": {
    "signingConfigs": [],
    "products": [
      {
        "name": "default",
        "signingConfig": "default",
        "compatibleSdkVersion": "5.0.1(13)",
        "runtimeOS": "HarmonyOS",
        "buildOption": {
          "strictMode": {
            "caseSensitiveCheck": true,
            "useNormalizedOHMUrl": true
          }
        }
      }
    ],
    "buildModeSet": [
      {
        "name": "debug",
      },
      {
        "name": "release"
      }
    ]
  },
  "modules": [
    {
      "name": "default",
      "srcPath": "./products/default",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    },
    {
      "name": "notes",
      "srcPath": "./features/notes"
    },
    {
      "name": "todos",
      "srcPath": "./features/todos"
    },
    {
      "name": "my",
      "srcPath": "./features/my"
    },
    {
      "name": "utils",
      "srcPath": "./commons/utils",
    },
    {
      "name": "uicomponents",
      "srcPath": "./commons/uicomponents",
    }
  ]
}